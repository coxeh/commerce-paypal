{
  "name": "craftcms/commerce-paypal",
  "description": "PayPal integration for Craft Commerce 2",
  "version": "2.1.0.1",
  "type": "craft-plugin",
  "keywords": [
    "paypal",
    "commerce",
    "craftcms",
    "omnipay",
    "yii2"
